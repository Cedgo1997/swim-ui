            "rawdescription": "\n",
            "type": "component",
            "sourceCode": "import { Component, EventEmitter, Input, Output } from '@angular/core';\n\n@Component({\n  selector: 'sui-button',\n  templateUrl: './button.component.html',\n  styleUrls: ['./button.component.scss']\n})\nexport class ButtonComponent {\n  @Output() onClick: EventEmitter<boolean> = new EventEmitter<boolean>();\n  @Input() disabled = false;\n  @Input() label = \"\";\n  type: string = 'gradient';\n\n  btnClicked(): void {\n    this.onClick.emit(true);\n  }\n}\n",
            "assetsDirs": [],
            "styleUrlsData": [
                {
                    "data": "@import \"node_modules/@fontsource/roboto\";\r\n@import './../../styles/colors';\r\n@import './../../styles/backgrounds';\r\n\r\n%btn-base {\r\n    border: none;\r\n    min-width: 70px;\r\n    padding: 0 12px 0 12px;\r\n    height: 30px;\r\n    font-weight: 500;\r\n    border-radius: 15px;\r\n    letter-spacing: 0.8px;\r\n    font-family: 'Roboto', sans-serif;\r\n    cursor: pointer;\r\n}\r\n\r\n.sui-btn {\r\n    @extend %btn-base;\r\n    box-shadow: 0px 4px 9px -5px rgba(0, 0, 0, 0.1);\r\n    transition: background-color 0.2s ease-in-out;\r\n\r\n    &--solid {\r\n        color: $white;\r\n        background-color: $aqua-500;\r\n\r\n        &:hover {\r\n            background-color: $aqua-600;\r\n        }\r\n\r\n        &:active {\r\n            background-color: $aqua-700;\r\n        }\r\n\r\n        &:disabled {\r\n            color: $aqua-700;\r\n            background-color: $aqua-200;\r\n            opacity: 0.5;\r\n        }\r\n    }\r\n\r\n    &--outline {\r\n        color: $aqua-700;\r\n        border: 1px solid $aqua-700;\r\n        background-color: transparent;\r\n\r\n        &:hover {\r\n            background-color: $aqua-50;\r\n        }\r\n\r\n        &:active {\r\n            background-color: $aqua-100;\r\n        }\r\n\r\n        &:disabled {\r\n            color: $aqua-700;\r\n            border: 1px solid $aqua-700;\r\n            background-color: transparent;\r\n            opacity: 0.5;\r\n        }\r\n    }\r\n\r\n    &--gradient {\r\n        background: linear-gradient(45deg, var(--aqua300), var(--aqua400), var(--aqua600));\r\n        color: $white;\r\n        transition: --aqua300 0.5s, --aqua400 0.6s, --aqua600 0.8s;\r\n\r\n        &:hover {\r\n            --aqua300: var(--aqua400);\r\n            --aqua400: var(--aqua500);\r\n            --aqua600: var(--aqua700);\r\n        }\r\n\r\n        &:active {\r\n            --aqua300: var(--aqua400);\r\n            --aqua400: var(--aqua600);\r\n            --aqua600: var(--aqua800);\r\n        }\r\n\r\n        &:disabled {\r\n            color: $aqua-700;\r\n            --aqua300: var(--aqua50);\r\n            --aqua400: var(--aqua100);\r\n            --aqua600: var(--aqua300);\r\n            opacity: 0.5;\r\n        }\r\n    }\r\n}",
                    "styleUrl": "./button.component.scss"
                }
            ],
            "stylesData": "",
            "templateData": "<button class=\"sui-btn\" [ngClass]=\"'sui-btn--' + type\" [disabled]=\"disabled\" (click)=\"btnClicked()\">\n    {{label}}\n</button>"
        },
